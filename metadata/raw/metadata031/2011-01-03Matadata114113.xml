          <title>
            <string language="en">Oil as fuel</string>
            <string language="x-mt-de">Öl als Kraftstoff</string>
            <string language="x-mt-fr">Huile comme carburant</string>
            <string language="x-mt-pt">Óleo como o combustível</string>
