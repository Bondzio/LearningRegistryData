          <title>
            <string language="en">Orbit of the Moon around Earth</string>
            <string language="x-mt-de">Bahn des Mondes um Masse</string>
            <string language="x-mt-fr">Orbite de la lune autour de la terre</string>
            <string language="x-mt-pt">Órbita da lua em torno da terra</string>
