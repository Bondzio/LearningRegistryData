          <title>
            <string language="en-gb">French: Ouverture</string>
            <string language="x-mt-de">Franzosen: Ouverture</string>
            <string language="x-mt-fr">Français : Ouverture</string>
            <string language="x-mt-pt">Francês: Ouverture</string>
