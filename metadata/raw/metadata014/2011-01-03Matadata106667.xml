          <title>
            <string language="en">Aluminium extraction</string>
            <string language="x-mt-de">Aluminiumextraktion</string>
            <string language="x-mt-fr">Extraction d'aluminium</string>
            <string language="x-mt-pt">Extração do alumínio</string>
