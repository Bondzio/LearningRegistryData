          <title>
            <string language="en">Gravitational field of Earth</string>
            <string language="x-mt-de">Gravitations fangen Sie von der Masse 
auf</string>
            <string language="x-mt-fr">Zone de la gravité de la terre</string>
            <string language="x-mt-pt">Campo gravitacional da terra</string>
            <string language="x-mt-it">Campo gravitazionale di terra</string>
