          <title>
            <string language="en">Listening Strategies:Travelling</string>
            <string language="x-mt-de">Hörendes Strategies:Travelling</string>
            <string language="x-mt-fr">Strategies:Travelling D'Écoute</string>
            <string language="x-mt-pt">Strategies:Travelling Escutando</string>
            <string language="x-mt-it">Strategies:Travelling Ascoltante</string>
