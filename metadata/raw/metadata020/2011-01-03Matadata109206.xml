          <title>
            <string language="en">Food pyramids</string>
            <string language="x-mt-de">Nahrungsmittelpyramiden</string>
            <string language="x-mt-fr">Pyramides de nourriture</string>
            <string language="x-mt-pt">Pirâmides do alimento</string>
            <string language="x-mt-it">Piramidi dell'alimento</string>
