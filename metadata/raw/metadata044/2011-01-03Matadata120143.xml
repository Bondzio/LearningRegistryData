          <title>
            <string language="en">Listening Strategies:Homework</string>
            <string language="x-mt-de">Hörendes Strategies:Homework</string>
            <string language="x-mt-fr">Strategies:Homework D'Écoute</string>
            <string language="x-mt-pt">Strategies:Homework Escutando</string>
            <string language="x-mt-it">Strategies:Homework Ascoltante</string>
            <string language="x-mt-el">Ακουσμα Strategies:Homework</string>
            <string language="x-mt-es">Strategies:Homework Que escucha</string>
          </title>
          <language>en</language>
          <description>
            <string language="en">This is a worksheet where English students improve their vocabulary related to homework ,words which they may also encounter in listening comprehension exercises. Includes some translation to hebrew</string>
          </description>
          <keyword>
            <string language="en">Listening comprehension</string>
          </keyword>
          <keyword>
            <string language="en">homework</string>
          </keyword>
