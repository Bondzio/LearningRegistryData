          <title>
            <string language="sl">Čebela  s pelodom</string>
            <string language="en">Bee with pollen</string>
            <string language="x-mt-de">Biene mit dem Blütenstaub</string>
            <string language="x-mt-fr">Abeille avec le pollen</string>
            <string language="x-mt-pt">Abelha com pólen</string>
