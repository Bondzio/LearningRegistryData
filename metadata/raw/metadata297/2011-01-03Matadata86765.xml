          <title>
            <string language="sl">Konično nihalo: Perioda in masa</string>
            <string language="en">Conical pendulum: Period and mass</string>
            <string language="x-mt-de">Konisches Pendel: Periode und Masse</string>
            <string language="x-mt-fr">Pendule conique : Période et masse</string>
