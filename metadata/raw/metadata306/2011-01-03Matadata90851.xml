          <title>
            <string language="en">Colour light</string>
            <string language="fi">Värivalo</string>
            <string language="x-mt-de">Farbe Licht</string>
            <string language="x-mt-fr">Lumière de couleur</string>
            <string language="x-mt-pt">Luz da cor</string>
