          <title>
            <string language="en">Large commercial generators</string>
            <string language="x-mt-de">Große kommerzielle Generatoren</string>
            <string language="x-mt-fr">Grands générateurs commerciaux</string>
            <string language="x-mt-pt">Geradores comerciais grandes</string>
