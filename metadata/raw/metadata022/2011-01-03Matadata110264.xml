          <title>
            <string language="en">Forces of gravity</string>
            <string language="x-mt-de">Kräfte von Schwerkraft</string>
            <string language="x-mt-fr">Forces de la pesanteur</string>
            <string language="x-mt-pt">Forças da gravidade</string>
