          <title>
            <string language="en">Listening Strategies:Pronouns</string>
            <string language="x-mt-de">Hörendes Strategies:Pronouns</string>
            <string language="x-mt-fr">Strategies:Pronouns D'Écoute</string>
            <string language="x-mt-pt">Strategies:Pronouns Escutando</string>
            <string language="x-mt-it">Strategies:Pronouns Ascoltante</string>
            <string language="x-mt-el">Ακουσμα Strategies:Pronouns</string>
            <string language="x-mt-es">Strategies:Pronouns Que escucha</string>
          </title>
          <language>en</language>
          <description>
            <string language="en">This is a worksheet where English students are taught to make correct use of pronouns which they may also encounter in listening comprehension exercises. Includes some translation to hebrew</string>
          </description>
          <keyword>
            <string language="en">Listening comprehension</string>
          </keyword>
          <keyword>
            <string language="en">pronouns</string>
          </keyword>
